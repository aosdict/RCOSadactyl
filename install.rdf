<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>pentadactyl@rcos.rpi.edu</em:id>
    <em:name>Pentadactyl</em:name>
    <em:description>Firefox, without a mouse</em:description>
    <em:version>0.2</em:version>
    <em:homepageURL>https://github.com/aosdict/RCOSadactyl</em:homepageURL>
    
    <!-- declare this add-on as an extension -->
    <em:type>2</em:type>
    <!-- is add-on bootstrappable? -->
    <em:bootstrap>true</em:bootstrap>
    <!-- always disable add-on when Firefox version is greater than the
	 max version specified below -->
    <em:strictCompatibility>false</em:strictCompatibility>

    <em:creator>Kris Maglione, Doug Kearns</em:creator>
    <em:developer>Cameron Root</em:developer>
    <em:developer>Ruben Madera</em:developer>
    
    <!-- The target application is Mozilla Firefox -->
    <em:targetApplication>
      <Description>
	<!-- Unique identifier for Firefox, DO NOT CHANGE -->
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
	<!-- Set min and max versions of Firefox this is compatible with -->
        <em:minVersion>31.0</em:minVersion>
        <em:maxVersion>36.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
