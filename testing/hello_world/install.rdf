<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-sntax-ns#"
	xmlns:em="http://www.mozilla.org/2004/em-rdf#">

<Description about="urn:mozilla:install-manifest">
	<em:id>hello_world@github.com</em:id> <!-- Unique identifier -->
	<em:name>Hello World</em:name> <!-- Name -->
	<em:description>A simple 'hello world' extension</em:description> <!-- Description -->
	<em:version>1.0</em:version> <!-- Version # -->
	<em:creator>RCOSadactyl dev team</em:creator> <!-- Who made it -->
	<em:homepageURL>http://www.github.com/gregbartell/RCOSadactyl</em:homepageURL> <!-- Where it's hosted -->

	<em:type>2</em:type> <!-- Signifies that this is an extension -->

	<em:targetApplication> <!-- What program does this run on? -->
		<Description>
			<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox's UUID -->
			<em:minVersion>31.0</em:minVersion> <!-- Lowest version number it will run on -->
			<em:maxVersion>35.*</em:maxVersion> <!-- Maximum version number it will run on -->
		</Description>
	</em:targetApplication>
</Description>
</RDF>
